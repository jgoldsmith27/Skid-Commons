    "@skid/shared": "0.1.0",
    "dotenv": "^16.4.7",
    "fastify": "^4.28.1",
    "fastify-socket.io": "^5.1.0",
    "openai": "^4.79.1",
    "socket.io": "^4.8.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "prisma": "^6.2.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
